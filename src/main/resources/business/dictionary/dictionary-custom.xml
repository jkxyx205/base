<?xml version="1.0" encoding="UTF-8"?>
<dictionary-config>
	<!--自定义字典，没有形成数据库字典-->
	<custom>
		<key>STATUS</key>
		<description>XXXX</description>
		<!-- 自定义的映射 -->
		<map>
	      	<entry key="ACTIVE">status.active</entry>
	      	<entry key="CLOSED">status.closed</entry>
        </map>
		<column-pattern>flag</column-pattern>
	</custom>
	<custom>
		<key>JOB_STATUS</key>
		<description>XXXX</description>
		<!-- 自定义的映射 -->
		<map>
	      	<entry key="0">Pause</entry>
	      	<entry key="1">Resume</entry>
        </map>
		<column-pattern>JOB_STATUS</column-pattern>
	</custom>
	
	
</dictionary-config>
